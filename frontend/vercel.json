{
    "version": 2,
    "builds": [
        {
            "src": "webServer.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/socket.io/(.*)",
            "dest": "webServer.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "webServer.js"
        },
        {
            "src": "/(.*)",
            "dest": "public/$1"
        }
    ]
}